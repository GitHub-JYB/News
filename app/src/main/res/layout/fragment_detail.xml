    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="admin_jyb.news.news.DetailFragment">



    <WebView
        android:id="@+id/webview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <ProgressBar
